[32m+[m[32m    "filas = cursor.fetchall()\n",[m
[32m+[m[32m    "column_names = [column[0] for column in cursor.description] # obtiene el nombre de las columnas\n",[m
[32m+[m[32m    "consulta = [list(fila) for fila in filas] # como los resultados vienen en tuplas, en este caso se crea un ciclo para recorrer cada una de ellas y luego crear el DF\n",[m
[32m+[m[32m    "\n",[m
[32m+[m[32m    "\n",[m
[32m+[m[32m    "Notas = pd.DataFrame(consulta, columns = column_names)\n",[m
[32m+[m[32m    "\n",[m
[32m+[m[32m    "Notas.head()"[m
[32m+[m[32m   ][m
[32m+[m[32m  },[m
[32m+[m[32m  {[m
